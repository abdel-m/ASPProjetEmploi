@model IEnumerable<EmploiASP.Contrat>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.dateEntree)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.dateSortie)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EntrepriseClient.denomination)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContratTravailleurNonSoumis.idContrat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContratTravailleurSoumis.numDossierMedical)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Profession.codeAlphabetique)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Travailleur.nom)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) 
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.dateEntree)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dateSortie)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EntrepriseClient.denomination)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContratTravailleurNonSoumis.idContrat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContratTravailleurSoumis.numDossierMedical)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Profession.codeAlphabetique)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Travailleur.nom)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idContrat }) |
            @Html.ActionLink("Details", "Details", new { id=item.idContrat }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idContrat })
        </td>
    </tr>
}

</table>
